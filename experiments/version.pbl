##This is an experiment for running likert-type or yes-no type questionnaires
 
define Start(par) {

  if(IsList(par))
   {
      subnum <- First(par)
   } else {

    subnum <- 0
   }

  win <- MakeWindow()
  font <- MakeFont("Vera.ttf",0,16, MakeColor("black"), MakeColor("white"),1)
  lab <- MakeLabel(GetPEBLVersion(),font)
  Print(GetPEBLVersion())
  AddObject(lab,win)
  Move(lab, 300,200)
  Draw()
  WaitForAnyKeyPress()

  
}

